{
  "name": "core-build-library",
  "description": "Core Build Library",
  "version": "1.0.0",
  "author": "Core Team",
  "bin": {
    "core-build": "./bin/run"
  },
  "bugs": "https://github.com/opensesame/core-build-library/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "current-git-branch": "^1.1.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/current-git-branch": "^1.1.1",
    "@types/node": "^10.17.51",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/opensesame/core-build-library",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "core-build",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf oclif.manifest.json && rimraf lib",
    "compile": "tsc -b && oclif-dev manifest && oclif-dev readme"
  },
  "repository": "opensesame/core-build-library",
  "types": "lib/index.d.ts"
}
